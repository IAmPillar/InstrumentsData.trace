#import <UIKit/UIKit.h>

/**
 *符号键盘view
 *注意：除了删除键（一部分），锁定功能，在当前页面直接操作外。
 *     其余所有按钮的点击事件都回调给父视图操作。
 ***/


@interface GZSymbolKeyboard : GZKeyboardBackgroundeView
/**
 * kenboardType 常用符号 显示类型  0常用 1中文 2英文 3网络 4单位
 ***/
- (instancetype)initWithFrame:(CGRect)frame andKeyboardType:(int)keyboardType;

@property (nonatomic, copy) void(^sendSelectedStr)(NSString* text);
@property (nonatomic, copy) void(^sendSelectedFunc)(int funcType); //功能按键返回 1删除  2符  3回车  4返回
@property (nonatomic, copy) void(^sendDeleteTap)(BOOL isDelete); //删除功能键 返回
/**
 *切换横竖屏时，传入的新高度
 ***/
- (void)changeViewFrame:(CGRect)newFrame;
- (void)changeBackgroudColor;
- (void)stopTimer; //手动停止计时器
@end
