#import <Foundation/Foundation.h>

/**
 *键盘皮肤相关设置、存读
 ***/

@interface GZKeyboardSkin : NSObject
//键盘背景相关
@property (nonatomic,strong) NSString *backgroudImage_kb;   //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_kb; //背景颜色 nil表示为透明
//导航相关
@property (nonatomic,strong) NSString *backgroudImage_nav; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_nav; //背景颜色 全键盘 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_nav;     //父视图 阴影颜色 nil表示没有阴影
//悬浮视图相关 如符号键盘左侧的滚动视图
@property (nonatomic,strong) NSString *backgroudImage_suspend; //背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_suspend; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_suspend;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_suspend;     //按钮 圆角 0表示不带圆角
@property (nonatomic,strong) NSString *backgroudImage_suspend_l; //左侧 背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_suspend_l; //左侧 背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_suspend_l;     //左侧 阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_suspend_l;     //按钮 圆角 0表示不带圆角
@property (nonatomic,strong) NSString *backgroudImage_suspend_b; //底部 背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_suspend_b; //底部 背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_suspend_b;     //底部 阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_suspend_b;     //按钮 圆角 0表示不带圆角
//候选框相关
@property (nonatomic,strong) NSString *backgroudImage_cd; //父视图 正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_cd; //父视图 背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_cd;     //父视图 阴影颜色 nil表示没有阴影
@property (nonatomic,strong) UIColor *backgroungColor_cd_subs;//子父视图 背景颜色 nil表示没有阴影
@property (nonatomic,strong) NSString *hightlightImage_cd_btn;//按钮 高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_cd_btn; //按钮 正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_cd_btn; //按钮 背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_cd_btn;     //按钮 阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_cd_btn;     //按钮 圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_cd_btn;      //按钮 字体颜色 不能nil
//按钮相关
@property (nonatomic,strong) NSString *hightlightImage_btn;//高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_btn; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_btn; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_btn;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_btn;     //圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_btn;      //字体颜色 不能nil
//功能按钮相关
@property (nonatomic,strong) NSString *hightlightImage_btn_f;//高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_btn_f; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_btn_f; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_btn_f;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_btn_f;     //圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_btn_f;      //字体颜色 不能nil
//功能按键 手写键盘左侧
@property (nonatomic,strong) NSString *hightlightImage_btn_wright_l;//高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_btn_wright_l; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_btn_wright_l; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_btn_wright_l;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_btn_wright_l;     //圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_btn_wright_l;      //字体颜色 不能nil
//功能按键 符号键盘左侧
@property (nonatomic,strong) NSString *hightlightImage_btn_symbol_l;//高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_btn_symbol_l; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_btn_symbol_l; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_btn_symbol_l;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_btn_symbol_l;     //圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_btn_symbol_l;      //字体颜色 不能nil
//功能按键 符号键盘底端
@property (nonatomic,strong) NSString *hightlightImage_btn_symbol_b;//高亮状态背景图 nil表示不设置
@property (nonatomic,strong) NSString *backgroudImage_btn_symbol_b; //正常状态背景图 nil表示不设置
@property (nonatomic,strong) UIColor *backgroungColor_btn_symbol_b; //背景颜色 nil表示为透明
@property (nonatomic,strong) UIColor *shadowColor_btn_symbol_b;     //阴影颜色 nil表示没有阴影
@property (nonatomic,assign) CGFloat cornerRadius_btn_symbol_b;     //圆角 0表示不带圆角
@property (nonatomic,strong) UIColor *titleColor_btn_symbol_b;      //字体颜色 不能nil
+ (id)defaultKeyboardSkin;
- (void)setSkin:(NSString*)skin;
@end
